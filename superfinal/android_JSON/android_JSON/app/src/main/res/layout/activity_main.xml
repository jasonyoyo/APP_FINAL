                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <EditText
                        android:id="@+id/editText_ca"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="20sp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/checkBox_ti"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Ti" />

                    <Spinner
                        android:id="@+id/spinner_ti"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <EditText
                        android:id="@+id/editText_ti"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="20sp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/checkBox_cr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Cr" />

                    <Spinner
                        android:id="@+id/spinner_cr"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <EditText
                        android:id="@+id/editText_cr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="20sp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/checkBox_mn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="mn" />

                    <Spinner
                        android:id="@+id/spinner_mn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

